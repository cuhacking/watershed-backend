{
  "name": "hackathon-project",
  "version": "1.0.0",
  "description": "cuHacking Hackathon Project",
  "scripts": {
    "build": "yarn lint && yarn compile",
    "compile": "yarn tsc",
    "dev": "nodemon -L -e ts --watch src --exec 'yarn compile && yarn serve'",
    "serve": "yarn node dist/server.js",
    "lint": "yarn tsc --noEmit && yarn eslint . --ext .ts,.js --fix",
    "start": "yarn build && yarn serve"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4",
    "@types/morgan": "^1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "@yarnpkg/pnpify": "^2.1.0",
    "eslint": "^7.5.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "ts-node": "^8.10.2"
  }
}
