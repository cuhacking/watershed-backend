{
  "name": "hackathon-project",
  "version": "1.0.0",
  "description": "cuHacking Hackathon Project",
  "scripts": {
    "build": "yarn lint && yarn compile",
    "compile": "yarn tsc",
    "dev": "nodemon -L -e ts --watch src --exec 'yarn compile && yarn serve'",
    "serve": "yarn node dist/server.js",
    "lint": "yarn tsc --noEmit && yarn eslint . --ext .ts,.js --fix",
    "start": "yarn build && yarn serve",
    "test": "yarn mocha -r ts-node/register ./tests/*.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/express": "^4",
    "@types/mocha": "^8.0.3",
    "@types/morgan": "^1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "@yarnpkg/pnpify": "^2.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.25"
  }
}
